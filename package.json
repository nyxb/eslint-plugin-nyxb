{
  "name": "eslint-plugin-nyxb",
  "type": "module",
  "version": "1.1.1",
  "packageManager": "pnpm@9.4.0",
  "description": "Nyxb extended ESLint rules",
  "author": "Dennis Ollhoff <contact@nyxb.email>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nyxb",
  "homepage": "https://github.com/nyxb/eslint-plugin-nyxb#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyxb/eslint-plugin-nyxb.git"
  },
  "bugs": "https://github.com/nyxb/eslint-plugin-nyxb/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "release": "bumping && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
   "eslint": "*"
 },
 "dependencies": {
   "@nyxb/utils": "^0.0.4"
 },
 "devDependencies": {
   "@nyxb/eslint-config": "^3.0.1",
   "@nyxb/nyxi": "^1.0.0",
   "@types/eslint": "^8.56.10",
   "@types/lodash.merge": "^4.6.9",
   "@types/node": "^20.14.9",
   "@types/semver": "^7.5.8",
   "@typescript-eslint/typescript-estree": "^7.15.0",
   "@typescript-eslint/utils": "^7.15.0",
   "bumping": "^9.2.1",
   "eslint": "^9.6.0",
   "eslint-define-config": "^2.1.0",
   "eslint-vitest-rule-tester": "^0.3.2",
   "esno": "^4.7.0",
   "jsonc-eslint-parser": "^2.4.0",
   "lint-staged": "^15.2.7",
   "lodash.merge": "4.6.2",
   "pnpm": "^9.4.0",
   "rimraf": "^5.0.7",
   "semver": "^7.6.2",
   "simple-git-hooks": "^2.11.1",
   "tsup": "^8.1.0",
   "tsx": "^4.16.0",
   "typescript": "^5.5.3",
   "unbuild": "^2.0.0",
   "vite": "^5.3.2",
   "vitest": "^1.6.0"
 },
 "resolutions": {
   "eslint-plugin-nyxb": "workspace:*"
 }
}
