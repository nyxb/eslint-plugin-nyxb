{
   "name": "eslint-plugin-nyxb",
   "type": "module",
   "version": "1.0.0-beta.7",
   "packageManager": "pnpm@8.8.0",
   "description": "Nyxb's extended ESLint rules",
   "author": "Dennis Ollhoff <contact@nyxb.email>",
   "license": "MIT",
   "funding": "https://github.com/sponsors/nyxb",
   "homepage": "https://github.com/nyxb/eslint-plugin-nyxb#readme",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/nyxb/eslint-plugin-nyxb.git"
   },
   "bugs": "https://github.com/nyxb/eslint-plugin-nyxb/issues",
   "keywords": [],
   "sideEffects": false,
   "exports": {
      ".": {
         "import": "./dist/index.mjs",
         "require": "./dist/index.cjs"
      }
   },
   "main": "./dist/index.mjs",
   "module": "./dist/index.mjs",
   "files": [
      "dist"
   ],
   "scripts": {
      "build": "unbuild",
      "dev": "unbuild --stub",
      "lint": "pnpm run dev && eslint .",
      "prepublishOnly": "nyxr build",
      "release": "bumpp && npm publish",
      "start": "esno src/index.ts",
      "test": "vitest",
      "typecheck": "tsc --noEmit",
      "prepare": "simple-git-hooks"
   },
   "peerDependencies": {
      "eslint": "*"
   },
   "dependencies": {
      "@typescript-eslint/utils": "^6.7.4"
   },
   "devDependencies": {
      "@nyxb/eslint-config": "^1.0.0-beta.6",
      "@nyxb/nyxi": "^0.0.31",
      "@nyxb/utils": "^0.0.3",
      "@types/eslint": "^8.44.3",
      "@types/lodash.merge": "^4.6.7",
      "@types/node": "^20.8.3",
      "@types/semver": "^7.5.3",
      "@typescript-eslint/rule-tester": "^6.7.4",
      "@typescript-eslint/typescript-estree": "^6.7.4",
      "@typescript-eslint/utils": "^6.7.4",
      "ajv": "^6.10.0",
      "bumpp": "^9.2.0",
      "eslint": "^8.51.0",
      "eslint-define-config": "^1.23.0",
      "esno": "^0.17.0",
      "lint-staged": "^14.0.1",
      "lodash.merge": "4.6.2",
      "pnpm": "^8.8.0",
      "rimraf": "^5.0.5",
      "semver": "^7.5.4",
      "simple-git-hooks": "^2.9.0",
      "tsup": "^7.2.0",
      "typescript": "^5.2.2",
      "unbuild": "^2.0.0",
      "vite": "^4.4.11",
      "vitest": "^0.34.6"
   },
   "resolutions": {
      "eslint-plugin-nyxb": "workspace:*"
   },
   "simple-git-hooks": {
      "pre-commit": "pnpm lint-staged"
   },
   "lint-staged": {
      "*": "eslint --fix"
   }
}
